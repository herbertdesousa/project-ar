{
  "expo": {
    "name": "project-ar",
    "slug": "project-ar",
    "version": "1.0.0",
    "scheme": "home",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
